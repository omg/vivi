{
  "compilerOptions": {
    "target": "esnext",
    "composite": true,

    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "strictNullChecks": true,

    "noEmit": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
